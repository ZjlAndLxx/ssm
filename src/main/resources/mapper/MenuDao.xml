<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.ssm.dao.MenuDao">
    <select id="queryAllTopMenu" resultType="Menu">
        SELECT
        *
        FROM
        `menu`
        WHERE
        parent_menu_id = -1L
    </select>
</mapper>